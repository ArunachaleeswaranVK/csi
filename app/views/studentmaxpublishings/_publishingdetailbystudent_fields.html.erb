
<div class="row">
<div class="nested-fields">
    

<div class="form-group col-md-4">
        <%= f.label :no_of_articles_published %>
        <%= f.text_field :no_of_articles_published , class: "form-control" %>
</div>
<div class="form-group col-md-4">
        <%= f.label :article_title %>
        <%= f.text_field :article_title , class: "form-control" %>
</div>
<div class="form-group col-md-3">
        <%= f.label :published_in %>
        <%= f.text_field :published_in , class: "form-control" %>
</div>
<div class="form-group col-md-4">
        <%= f.label :date_of_publishing , "No of students from Abroad / Country Name" %>
        <%= f.date_field :date_of_publishing , class: "form-control" %>
</div>


<div class="form-group col-md-1 remove-sub-form-button">
   <%= link_to_remove_association "Remove", f, class: "form-button btn btn-danger" %> 
</div>



</div> 

</div>