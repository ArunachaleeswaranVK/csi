<div class="row">
<div class="nested-fields">
    
<!--add a drop down -->
<div class="form-group sf col-md-3">
        
        <%= f.text_field :year ,type: "text",  required: true , class: "form-control"%>
</div>
<div class="form-group sf col-md-3">
       
        <%= f.text_field :institution_name ,  required: true, class: "form-control"%>
</div>
<div class="form-group sf col-md-3">
        
        <%= f.text_field :imno_of_institution ,  required: true, class: "form-control" %>
</div>
<div class="form-group sf col-md-2">
       
        <%= f.text_field :volunteers, required: true , type: "number",
      min: "0" ,max: "100", maxlength: "3" , class: "form-control"%>
</div>

<div class="form-group sf col-md-1 remove-sub-form-button">
   <%= link_to_remove_association "Remove", f, class: "form-button btn btn-danger" %> 
</div>

</div>



</div>