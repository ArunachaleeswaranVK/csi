        <%= f.label :year %>
        <%= f.text_field :year , class: "form-control"%>
</div>
<div class="form-group col-md-3">
        <%= f.label :institution_name %>
        <%= f.text_field :institution_name , class: "form-control"%>
</div>
<div class="form-group col-md-3">
        <%= f.label :imno_of_institution %>
        <%= f.text_field :imno_of_institution, class: "form-control" %>
</div>
