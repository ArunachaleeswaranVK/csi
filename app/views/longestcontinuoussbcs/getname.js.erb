$('#imno').val('<%= @longestcontinuoussbc.imno %>');
$('#address').val('<%= @longestcontinuoussbc.address %>');
<% unless @longestcontinuoussbc.district.empty? %>
  $('#district').val('<%= @longestcontinuoussbc.district %>');
<% else %>  
  $('#district').val('Not Available');   
<% end %>    
$('#city').val('<%= @longestcontinuoussbc.city %>');
$('#state').val('<%= @longestcontinuoussbc.state %>');
$('#pin').val('<%= @longestcontinuoussbc.pincode %>');
$('#sbc-name').val('<%= @longestcontinuoussbc.sbcs.first.sbc_name %>');
<% unless @longestcontinuoussbc.district.empty? %>
  $('#sbc-imno').val('<%= @longestcontinuoussbc.sbcs.first.sbc_membership_no %>');
<% else %>  
  $('#sbc-imno').val('Not Available');   
<% end %>  

$('#sbc-email').val('<%= @longestcontinuoussbc.sbcs.first.sbc_email %>');
$('#sbc-phone').val('<%= @longestcontinuoussbc.sbcs.first.sbc_phone %>');
