
    <div>
       <h1 class="show-head">Paper Presenter at International Conference for Faculty Award</h1>
       <p class="show-submitted-by">Submitted by: <%= @paperpresenter.user.email %></p>
    </div>
    

   
  
    
     <!--form specific details-->
    
     <div class="row">
             <br></br>
      <p class="primary-form-subheading">Faculty Details</p>  
    <div class="form-group col-md-3">
     <label>Faculty Name</label>
     <input type="text" class="form-control" id="name" value="<%= @paperpresenter.faculty_name %>" >   
    </div>
    <div class="form-group col-md-2">
     <label>Faculty Number</label>
     <input type="text" class="form-control" id="name" value="<%= @paperpresenter.faculty_number %>" >   
    </div>
    <div class="form-group col-md-2">
      <label>Csi membership No</label>
     <input type="text" class="form-control" id="name" value="<%= @paperpresenter.faculty_csi_membership_no %>" >   
    </div>
    <div class="form-group col-md-2">
      <label>Valid Till</label>
     <input type="text" class="form-control" id="name" value="<%= @paperpresenter.valid_till %>" >   
    </div>
    <div class="form-group col-md-3">
      <label>Faculty Email</label>
     <input type="text" class="form-control" id="name" value="<%= @paperpresenter.faculty_email %>" >   
    </div>
   </div>
   <div class="row">
       <br></br>
    <p class="primary-form-subheading">Conference Details:</p>  
    
       
    
      <div class="col-md-3"><p class="subform-head"><strong>Name</strong></p></div>
      <div class="col-md-2"><p  class="subform-head"><strong>Date</strong></p></div>
      <div class="col-md-2"><p><strong>Organised by</strong></p></div>
      <div class="col-md-3"><p><strong>Place</strong></p></div>
      <div class="col-md-2"><p><strong>India or Outside</strong></p></div>
    <% @paperpresenter.paperpresenterdetails.each do |i| %>
         
         




<div class="nested-fields">

<div class="form-group sf col-md-3">
        
        <p><%= i.name_of_conference %></p>
</div>

<div class="form-group sf col-md-2">
        
      <p><%= i.date_of_conference %></p>
</div>

<div class="form-group sf col-md-2">
        
        
         <p><%= i.organised_by %></p>
</div>
<div class="form-group sf col-md-3">
        
        
         <p><%= i.name_place_of_conference %></p>
</div>
<div class="form-group sf col-md-2">
        
        
         <p><%= i.india_or_outside %></p>
</div>

</div> 


      <% end %>
    </div>
   
    <br></br>
    
    
     <div>
  
  <%= filepicker_save_button "Save Attachment", @paperpresenter.attachment , "application/zip, application/octet-stream" %>
</div>  
    