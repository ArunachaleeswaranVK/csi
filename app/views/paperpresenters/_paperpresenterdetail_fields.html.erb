<div class="row">
<div class="nested-fields">
    

<div class="form-group sf col-md-2">
        
        <%= f.text_field :name_of_conference , required: true , class: "form-control"%>
</div>
<div class="form-group sf col-md-2">
       
        <%= f.date_field :date_of_conference , required: true  , class: "form-control"%>
</div>
<div class="form-group sf col-md-3">
        
        <%= f.text_field :organised_by , required: true , class: "form-control" %>
</div>
<div class="form-group sf col-md-3">
       
        <%= f.text_field :name_place_of_conference , required: true , class: "form-control"%>
</div>
<div class="form-group sf col-md-1">
       
       
        <%= f.select(:india_or_outside, options_for_select([['India', "India"], 
        ['Aborad', "Abroad"]]), {}, {class: "form-control"}) %>
</div>
<div class="form-group sf col-md-1 remove-sub-form-button">
   <%= link_to_remove_association "Remove", f, class: "form-button btn btn-danger" %> 
</div>

</div>
</div>