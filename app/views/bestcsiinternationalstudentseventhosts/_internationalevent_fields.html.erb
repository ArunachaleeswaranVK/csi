

<div class="row">



<div class="nested-fields">

<div class="form-group sf col-md-3">
        
        <%= f.text_field :competetion_name ,  required: true , class: "form-control" %>
</div>

<div class="form-group sf col-md-3">
        
      <%= f.date_field :date_of_event ,  required: true , class: "form-control" %>
</div>

<div class="form-group sf col-md-2">
        
        
         <%= f.select(:no_of_students_from_abroad, options_for_select([
          ['10-25', '10-25'], 
          ['26-50', '26-50'],
          ['51-75','51-75'], 
          ['76-100', '76-100'], 
          ['>100', '>100']]), {}, {class: "form-control"}) %>
</div>
<div class="form-group sf col-md-3">
        
        <%= f.text_field :country_name  ,  required: true, class: "form-control" %>
</div>


<div class="form-group sf col-md-1 remove-sub-form-button">
   <%= link_to_remove_association "Remove", f, class: "form-button btn btn-danger btn-md" %>
</div>



</div> 
</div>
