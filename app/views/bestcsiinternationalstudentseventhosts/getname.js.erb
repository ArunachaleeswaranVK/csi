$(function() {
    var institute = <%Institute.all.collect{|institute| institute.name}%>
    $( "#name" ).autocomplete({
      source: institutes
    });
}); 


<!--instution details-->

$('#imno').val('<%= @bestcsiinternationalstudentseventhost.imno %>');

<% unless @bestcsiinternationalstudentseventhost.address.nil? %>
  $('#address').val('<%= @bestcsiinternationalstudentseventhost.address %>');
<% else %>  
  $('#address').val('Not Available');   
<% end %>



<% unless @bestcsiinternationalstudentseventhost.city.nil? %>
  $('#city').val('<%= @bestcsiinternationalstudentseventhost.city %>');
<% else %>  
  $('#city').val('Not Available');   
<% end %>

<% unless @bestcsiinternationalstudentseventhost.state.nil? %>
  $('#state').val('<%= @bestcsiinternationalstudentseventhost.state %>');
<% else %>  
  $('#state').val('Not Available');   
<% end %>



<!--nominating authority-->

<% unless @bestcsiinternationalstudentseventhost.nominating_authority.nil? %>
 $('#na').val('<%= @bestcsiinternationalstudentseventhost.nominating_authority %>');
<% else %>  
  $('#na').val('Not Available');   
<% end %> 

 





<!--sbc details-->

<!--check if the institute has sbc association-->
<% unless @bestcsiinternationalstudentseventhost.sbcs.empty? %>
  <% unless @bestcsiinternationalstudentseventhost.sbcs.first.nil? %>
    $('#sbc-name').val('<%= @bestcsiinternationalstudentseventhost.sbcs.first.sbc_name %>');
  <% else %>  
    $('#sbc-name').val('Not Available');   
  <% end %>
  
  
  
  <% unless @bestcsiinternationalstudentseventhost.sbcs.first.sbc_email.nil? %>
    $('#sbc-email').val('<%= @bestcsiinternationalstudentseventhost.sbcs.first.sbc_email %>');
  <% else %>  
    $('#sbc-email').val('Not Available');   
  <% end %> 
  
  
  
  <% unless @bestcsiinternationalstudentseventhost.sbcs.first.no_of_volunteers.nil? %>
    $('#volunteers').val('<%= @bestcsiinternationalstudentseventhost.sbcs.first.no_of_volunteers %>');
  <% else %>  
    $('#volunteers').val('Not Available');   
  <% end %> 

<% else %>
  $('#sbc-name').val('Not Available');
  $('#sbc-imno').val('Not Available');
  $('#sbc-email').val('Not Available'); 
  $('#sbc-phone').val('Not Available');
  $('#volunteers').val('Not Available'); 
<% end %>







